{
	"parserOptions": {
		"ecmaVersion": 6
	},

	"globals": {
		"app": true,
		"angular": true
	},

	"rules": {
		"no-console": 1,
		"no-debugger": 1,
		"no-extra-parens": 1,
		"no-irregular-whitespace": 1,
		"no-unreachable": 1,
		"use-isnan": 1,
		"valid-jsdoc": 1,
		"eqeqeq": 1,
		"no-alert": 1,
		"no-multi-spaces": 1,
		"no-native-reassign": 1,
		"no-octal": 1,
		"strict": 0,
		"no-undef": 1,
		"no-undef-init": 1,
		"no-undefined": 1,
		"no-unused-vars": 1,
		"camelcase": 1,
		"consistent-this": 1,
		"eol-last": 1,
		"no-nested-ternary": 0,
		"no-trailing-spaces": 1,
		"quotes": 0,
		"space-infix-ops": 1,
		"no-var": 1,
	}
}
